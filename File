				// some tools in scripts.js need the globals so we add them here
				var $GLOBALS =  {
					'cdn-user-files' : 'https://cdn-cms.f-static.net',
					'maps-display-domain' : 'https://maps.site123.com',
					'cdn-system-files' : '//app.site123.com',
